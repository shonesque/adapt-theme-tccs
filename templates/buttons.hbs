<div class="buttons-inner">
    <div class="buttons-cluster clearfix">
        <hr class="component">
        <button class="buttons-action" aria-label="{{_buttons._submit.ariaLabel}}">{{{_buttons._submit.buttonText}}}</button>
        {{#if _canShowModelAnswer}}
        <button class="buttons-feedback" aria-label="{{_buttons._showFeedback.ariaLabel}}" disabled="true">{{{_buttons._showFeedback.buttonText}}}</button>
        {{/if}}
    </div>
        {{#unless _shouldDisplayAttempts}}
    <div class="feedback-container">
        <div class="correct-marking-feedback display-none"></div>
        <div class="correct-marking-feedback-title display-none">{{{_CorrectMarkingFeedbackTitle}}}</div>
        
        <div class="incorrect-marking-feedback display-none"></div>
        <div class="incorrect-marking-feedback-title display-none">{{{_IncorrectMarkingFeedbackTitle}}}</div>
    
        <div class="partlycorrect-marking-feedback display-none"></div>
        <div class="partlycorrect-marking-feedback-title display-none">{{{_PartlyCorrectMarkingFeedbackTitle}}}</div>
        <div class="feedback-text display-none"></div>
    
    </div>
</div>
    {{/unless}}
    {{#if _shouldDisplayAttempts}}
    <div class="buttons-display">
        <div class="buttons-marking-icon icon display-none">
        </div>
        <div class="buttons-display-inner">
        </div>
    </div>
    {{/if}}
</div>